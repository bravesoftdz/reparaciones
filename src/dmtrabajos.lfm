  object qPendientes: TZQuery
    Connection = DM_General.cnxGeneral
    SQL.Strings = (
      'SELECT T.fIngreso as Ingreso'
      '      , T.Diagnostico as Diagnostico'
      '      , T.fEgreso as Egreso'
      '      , T.Detalle as Reparacion'
      '      , T.fCompromiso as Compromiso'
      '      , C.Nombre as Cliente'
      '      , C.Organizacion as Organismo'
      '      , C.documento as Documento'
      '      , C.Domicilio as Domicilio'
      '      , C.Telefono as Telefono'
      '      , C.email as email'
      '      , C.notas as NotasCliente'
      '      , E.modelo as Equipomodelo'
      '      , E.nSerie as EquipoSerie'
      '      , E.notas as EquipoNotas'
      '      , M.Marca as EquipoMarca'
      'FROM Trabajos T'
      '     LEFT JOIN Clientes C ON T.cliente_id = C.id'
      '     LEFT JOIN Equipos E ON T.equipo_id = E.id'
      '     LEFT JOIN Marcas as M ON E.marca_id = M.id'
      'WHERE (T.bVisible = 1)'
      '      and (T.fEgreso = 0)'
      'ORDER BY T.fIngreso'
    )
